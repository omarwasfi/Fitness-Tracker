@page "/MyProfile"
@inject IPersonService PersonService

<MudContainer>

    @if (person != null)
    {


        @if (!editMode)
        {
            <MudImage Src="@person.ProfilePicture.Base64" Width="200" Height="200" Alt="Swedish Farm House" Elevation="25" Class="rounded-lg ma-4"/>
            <h3>@person.UserName</h3>

            <p>@person.FirstName</p>
            <p>@person.LastName</p>

            <MudButton OnClick="switchEditModel" Variant="Variant.Filled" EndIcon="@Icons.Material.Filled.Edit" Color="Color.Primary">Edit</MudButton>

        }
        else
        {
            <MudGrid>
                <MudItem xs="12">
                    <MudImage Src="@person.ProfilePicture.Base64" Width="200" Height="200" Alt="Swedish Farm House" Elevation="25" Class="rounded-lg ma-4"/>
                </MudItem>
                <MudItem xs="12">
                    <InputFile id="fileInput" OnChange="UploadFiles" hidden multiple accept=".jpg, .jpeg, .png"/>

                    <MudFab HtmlTag="label"
                            Color="Color.Secondary"
                            Icon="@Icons.Filled.Image"
                            Label="Load new profile picture"
                            for="fileInput"/>
                </MudItem>
                <MudItem xs="12">
                    <MudButton OnClick="switchEditModel" Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Save" Color="Color.Info">Save</MudButton>
                </MudItem>

            </MudGrid>




        }

    }




</MudContainer>


@code {
        private PersonDataViewModel person = new PersonDataViewModel() { UserName = "", FirstName = "", LastName = "", ProfilePicture = new PictureBase64DataViewModel() {Base64 = "data:image/jpg;base64, " } };

    private bool editMode { get; set; } = false;

    /// <summary>
    /// Saving the picture in the index 0
    /// </summary>
    IList<IBrowserFile> files = new List<IBrowserFile>();


    protected override async Task OnParametersSetAsync()
    {
        person = await PersonService.GetTheAuthorizedPerson();
        editMode = false;
        StateHasChanged();
    }

    private async Task switchEditModel()
    {
        if (!editMode)
        {

            editMode = true;
        }
        else
        {

            editMode = false;
        }

        StateHasChanged();
    }

    private async Task UploadFiles(InputFileChangeEventArgs e)
    {
        foreach (var file in e.GetMultipleFiles())
        {
            files.Add(file);
        }

        this.person.ProfilePicture = await PersonService.UpdateProfilePicture(files[0]);

        files = new List<IBrowserFile>();
        StateHasChanged();
    }
}

